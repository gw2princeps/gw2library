{
  "name": "gw2builds",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "cfwatch": "npx @cloudflare/next-on-pages --watch",
    "cfdev": "npx wrangler pages dev .vercel/output/static --compatibility-flags=streams_enable_constructors",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "publishlambda": "cd lambda && zip --symlinks ../lambda.zip * && cd .. && aws lambda update-function-code --function-name mdxToString --zip-file fileb://lambda.zip && rm lambda.zip",
    "analyze": "ANALYZE=true yarn build"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.213.0",
    "@blueprintjs/core": "^4.12.0",
    "@discretize/gw2-ui-new": "^0.1.1",
    "@discretize/react-discretize-components": "^2.5.0",
    "@discretize/remark-mdx-gw2ui": "^0.0.3",
    "@discretize/typeface-menomonia": "^0.1.3",
    "@mdx-js/loader": "^2.1.5",
    "@mdx-js/mdx": "^2.1.5",
    "@mdx-js/react": "^2.1.5",
    "@next/bundle-analyzer": "^13.0.3",
    "@next/mdx": "^13.0.3",
    "@types/mdx": "^2.0.3",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.3",
    "gw2e-chat-codes": "^1.3.2",
    "next": "13.0.3",
    "normalize.css": "^8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remark-gfm": "^3.0.1",
    "sharp": "^0.31.2",
    "typeface-muli": "^1.1.13",
    "typeface-raleway": "^1.1.13",
    "typescript": "4.8.4",
    "usehooks-ts": "^2.9.1",
    "uuid": "^9.0.0"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^0.2.0",
    "@types/uuid": "^8.3.4",
    "vercel": "^28.5.4"
  }
}
